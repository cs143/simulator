<?xml version="1.0"?>
<Simulator>

  <Topology>
  <Hosts>
    <Host name="Host1"/>
    <Host name="Host2"/>
  </Hosts>
  <Routers>
    <Router name="R1" />
    <Router name="R2" />
  </Routers>
  <Links>
    <Link name="Link1" rate="50000" prop_delay="10" buffer_size="50" from="Host1" to="Host2"/>
  </Links>
  </Topology>
  <Flows>
    <Flow name="F1" from="Host1" to="Host2" pkt_count="1000000" start_time="10" algorithm="AIMD"/>
  </Flows>
  <LogFilePath path="log.txt" />
</Simulator>
