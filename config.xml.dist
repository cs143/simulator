<?xml version="1.0"?>
<Simulator>
  <Topology>
  <Hosts>
    <Host name="Host1"/>
    <Host name="Host2"/>
  </Hosts>
  <Routers>
    <Router name="R1" />
  </Routers>
  <Links>
    <Link name="Link1" rate="12500000" prop_delay="0.01" buffer_size="4" from="Host1" to="R1"/>
    <Link name="Link2" rate="10000000" prop_delay="0.01" buffer_size="4" from="R1" to="Host2"/>
  </Links>
  </Topology>
  <Flows>
    <Flow name="F1" from="Host1" to="Host2" pkt_count="6000000" start_time="10" algorithm="fasttcp"/>
  </Flows>
  <LogFilePath path="log.xml" />
</Simulator>
